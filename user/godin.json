{
  "docker": {
    "registry": "docker.coozzy.ch"
  },
  "godin": {
    "version": "v0.4.0"
  },
  "project": {
    "created": "Sun, 23 Jun 2019 10:15:02 CEST",
    "updated": "Tue, 25 Jun 2019 19:57:31 CEST"
  },
  "protobuf": {
    "package": "github.com/lukasjarosch/godin-examples/user/api",
    "service": "UserService"
  },
  "service": {
    "endpoints": {
      "create": {
        "protobuf": {
          "request": "CreateRequest",
          "response": "CreateResponse"
        }
      },
      "delete": {
        "protobuf": {
          "request": "DeleteRequest",
          "response": "EmptyResponse"
        }
      },
      "get": {
        "protobuf": {
          "request": "GetRequest",
          "response": "GetResponse"
        }
      },
      "list": {
        "protobuf": {
          "request": "ListRequest",
          "response": "ListResponse"
        }
      }
    },
    "middleware": {
      "authorization": false,
      "caching": false,
      "logging": true,
      "recovery": true
    },
    "module": "github.com/lukasjarosch/godin-examples/user",
    "name": "user",
    "namespace": "godin"
  },
  "transport": {
    "amqp": {
      "default_address": "amqp://username:password@host:port/vhost",
      "subscriber": {
        "user_created_subscriber": {
          "topic": "user.created",
          "exchange": "user-exchange",
          "exchange_type": "durable",
          "queue": "user-created-queue"
        }
      }
    },
    "grpc": {
      "enabled": true
    }
  }
}
